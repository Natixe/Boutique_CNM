{
  "name": "boutique-cnm",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "server/server.js",
  "scripts": {
    "client-dev": "vite",
    "server-dev": "nodemon server/server.js",
    "start": "concurrently \"npm run client-dev\" \"npm run server-dev\"",
    "prod": "npm run client-build && node server/server.js",
    "client-build": "vite build",
    "client-preview": "vite preview",
    "format": "npx prettier --write **/*.{js,jsx,md}",
    "format:check": "npx prettier --check **/*.{js,jsx,md}",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@paypal/paypal-js": "^8.1.0",
    "@paypal/react-paypal-js": "^8.5.0",
    "@react-spring/web": "^9.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^11.3.22",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "react-scripts": "5.0.1",
    "react-use-gesture": "^9.1.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "source-map-loader": "^1.0.0",
    "vite": "^5.3.5"
  }
}
